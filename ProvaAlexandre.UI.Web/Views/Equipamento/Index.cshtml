@model IEnumerable<ProvaAlexandre.Dominio.Equipamento>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Cadastrar Equipamento", "Cadastrar")
</p>
<table>
    <tr>
        <th>
            Codigo do produto
        </th>
        <th>
            Cliente Responsável
        </th>
        <th>
           Funcionário Responsável
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Marca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Modelo)
        </th>
        <th>
            Número de Série
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Equipamento_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cliente_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Funcionario_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Marca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Modelo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NSerie)
        </td>
        <td>
            @Html.ActionLink("Editar", "Editar", new { id=item.Equipamento_id}) |
            @Html.ActionLink("Detalhes", "Detalhes", new { id=item.Equipamento_id }) |
            @Html.ActionLink("Excluir", "Excluir", new { id=item.Equipamento_id })
        </td>
    </tr>
}

</table>
